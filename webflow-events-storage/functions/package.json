{
  "name": "webflow-events-storage",
  "version": "0.0.0",
  "description": "The Webflow Events Storage extension lets you sync Webflow data in your Google Firebase project.",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "build:watch": "npm run clean && tsc --watch",
    "clean": "rimraf lib",
    "compile": "tsc -p tsconfig.build.json",
    "local:emulator": "cd ../../_emulator && firebase emulators:start -P demo-test",
    "test": "jest",
    "test:local": "concurrently \"npm run local:emulator\" \"jest\"",
    "test:watch": "concurrently \"npm run local:emulator\" \"jest --watch\"",
    "generate-readme": "firebase ext:info .. --markdown > ../README.md"
  },
  "contributors": [
    {
      "name": "SimplyCubed",
      "email": "support@simplycubed.com",
      "url": "https://github.com/simplycubed"
    },
    {
      "name": "Anas Muhammed",
      "email": "anas.mu2022@gmail.com",
      "url": "https://github.com/anasmuhmd"
    }
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@simplycubed/webflow-utils": "^0.0.0",
    "@types/express-serve-static-core": "4.17.32",
    "@types/jest": "^29.2.5",
    "@types/supertest": "^2.0.12",
    "body-parser": "^1.20.1",
    "concurrently": "^7.6.0",
    "express": "^4.18.2",
    "firebase-admin": "^11.3.0",
    "firebase-functions": "^4.1.1",
    "firebase-tools": "^11.18.0",
    "jest": "^29.3.1",
    "lodash.chunk": "^4.2.0",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4",
    "webflow-api": "^1.2.2"
  },
  "engines": {
    "node": "16"
  },
  "private": true,
  "devDependencies": {
    "@types/lodash.chunk": "^4.2.7",
    "@types/node-fetch": "^2.6.2",
    "concurrency": "^0.1.4",
    "dotenv": "^16.0.3",
    "wait-port": "^1.0.4"
  }
}
